<project name="CustomBuilt360">
	<target name="checkout">
		<antcall target="_clear" />
		<antcall target="_required" />
		<antcall target="_testing" />
	</target>
	<target name="_clear">
		<delete dir="plugins" />
		<mkdir dir="plugins" />
	</target>
	<target name="_required">
		<osgi.release osgirepository="com.netappsid.tools.releaser.s3.S3OSGIRepository" dir="plugins">
			<plugin symbolicname="com.netappsid.javassist" version="3.9.0" />
			<plugin symbolicname="javax.persistence" version="1.99.0" />
		</osgi.release>
	</target>
	<target name="_testing">
		<osgi.release osgirepository="com.netappsid.tools.releaser.s3.S3OSGIRepository" dir="plugins">
			<plugin symbolicname="org.mockito.mockito-core-eclipse" version="1.8.5" />
			<plugin symbolicname="com.springsource.org.objenesis" version="1.2.0" />
		</osgi.release>
	</target>
</project>